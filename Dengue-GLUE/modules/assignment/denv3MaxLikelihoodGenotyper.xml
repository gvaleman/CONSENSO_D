<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>denv3MaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
	<useSingleReference>true</useSingleReference>

	<cladeCategory>
		<name>serotype</name>	
		<displayName>Serotype</displayName>	
		<whereClause>displayName = '3'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
		<distanceCutoff>0.4</distanceCutoff>
		<finalCladeCutoff>80.0</finalCladeCutoff>
	</cladeCategory>
	
	<cladeCategory>
		<name>genotype</name>	
		<displayName>Genotype</displayName>	
		<whereClause>displayName like '3%' and clade_category = 'genotype'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
		<distanceCutoff>0.06</distanceCutoff>
		<finalCladeCutoff>80.0</finalCladeCutoff>
	</cladeCategory>

	<cladeCategory>
		<name>major_lineage</name>	
		<displayName>Major lineage</displayName>	
		<whereClause>displayName like '3%' and clade_category = 'major_lineage'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.03</distanceCutoff>
		<finalCladeCutoff>70.0</finalCladeCutoff>		
	</cladeCategory>

	<cladeCategory>
		<name>minor_lineage</name>	
		<displayName>Minor lineage</displayName>	
		<whereClause>displayName like '3%' and clade_category = 'minor_lineage'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.018</distanceCutoff>
		<finalCladeCutoff>70.0</finalCladeCutoff>		
	</cladeCategory>

</maxLikelihoodGenotyper>



