<maxLikelihoodGenotyper>
	<maxLikelihoodPlacerModuleName>denv1MaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
	<useSingleReference>true</useSingleReference>

	<cladeCategory>
		<name>serotype</name>	
		<displayName>Serotype</displayName>	
		<whereClause>displayName = '1'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
		<distanceCutoff>0.4</distanceCutoff>
		<finalCladeCutoff>65.0</finalCladeCutoff>
	</cladeCategory>

	<cladeCategory>
		<name>genotype</name>	
		<displayName>Genotype</displayName>	
		<whereClause>displayName like '1%' and clade_category = 'genotype'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.1</distanceCutoff>
		<finalCladeCutoff>65.0</finalCladeCutoff>		
	</cladeCategory>
	
	<cladeCategory>
		<name>major_lineage</name>	
		<displayName>Major lineage</displayName>	
		<whereClause>displayName like '1%' and clade_category = 'major_lineage'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
		<distanceCutoff>0.04</distanceCutoff>
		<finalCladeCutoff>60.0</finalCladeCutoff>		
	</cladeCategory>

	<cladeCategory>
		<name>minor_lineage</name>	
		<displayName>Minor lineage</displayName>	
		<whereClause>displayName like '1%' and clade_category = 'minor_lineage'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.02</distanceCutoff>
		<finalCladeCutoff>60.0</finalCladeCutoff>		
	</cladeCategory>


</maxLikelihoodGenotyper>



