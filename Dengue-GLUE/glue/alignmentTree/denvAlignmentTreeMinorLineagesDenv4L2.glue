  #### Alignments representing and containing minor DENV4 sublineages

  alignment AL_DENV_4I_A1
  
    set field displayName "4I_A1.1"
    set field clade_category "minor_lineage" 

	extract child AL_DENV_4I_A1.1 -r REF_DENV_4I_A.1.1_MZ976860
	demote member AL_DENV_4I_A1.1 --whereClause "sequence.minor_sublineage = '4I_A.1.1'"	
  
	extract child AL_DENV_4I_A1.2 -r REF_DENV_4I_A.1.2_OQ427041
	demote member AL_DENV_4I_A1.2 --whereClause "sequence.minor_sublineage = '4I_A.1.2'"	

	exit


  alignment AL_DENV_4I_A1.1
    set field displayName "4I_A.1.1"
    set field clade_category "minor_sublineage" 
	exit

  alignment AL_DENV_4I_A1.2
    set field displayName "4I_A.1.2"
    set field clade_category "minor_sublineage" 
	exit

